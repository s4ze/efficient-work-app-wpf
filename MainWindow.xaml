<Window x:Class="EfficientWorkApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Name="Window" Title="Efficient Work App" Height="800" Width="800" ResizeMode="NoResize" Background="White">
    <Grid>
        <Grid HorizontalAlignment="Center" VerticalAlignment="Bottom">
            <StackPanel Height="50" Orientation="Horizontal">
                <Button Content="Pomodoro" Click="BtnClickPomodoroPage" Margin="5" Width="70"/>
                <Button Content="Water" Click="BtnClickWaterPage" Margin="5" Width="70"/>
                <Button Content="Warmup" Click="BtnClickWarmupPage" Margin="5" Width="70"/>
                <Button Content="Food" Click="BtnClickFoodPage" Margin="5" Width="70"/>
            </StackPanel>
        </Grid>
        <Frame x:Name="Main" Margin="5" NavigationUIVisibility="Hidden">
            
        </Frame>
    </Grid>
</Window>
